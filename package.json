    "@storybook/addon-links": "^6.0.13",
    "@storybook/react": "^6.0.13",
    "autoprefixer": "9.4.7",
    "babel-loader": "8.0.5",
    "cross-env": "5.2.0",
    "css-loader": "2.1.0",
    "lerna": "3.13.0",
    "node-sass": "^4.12.0",
    "postcss-loader": "3.0.0",
