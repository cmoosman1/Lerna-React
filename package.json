{
  "name": "components-library",
  "version": "1.0.0",
  "description": "A components library",
  "engines": {
    "node": ">=10.17.0"
  },
  "engineStrict": true,
  "main": "index.js",
  "scripts": {
    "build:components": "lerna bootstrap && lerna exec npm run \"build\"",
